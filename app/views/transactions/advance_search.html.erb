
<div id="rightColumn">
  <div class="contentbox">
    <h1>Transactions <span>(Showing all 17 transactions)</span></h1>
    <div class="box">
      <div class="bt">
        <div class="br">
          <div class="bb">
            <div class="bl">
              <div class="con">
                <h3>Email docs to your Working Docs page: <strong>2946-32647-x@docs.paperlesspipeline.com  Download</strong></h3>
                <div class="unreviewedBox page-transactionsN">
                  <div class="unreviewedBoxContent">
                    <input class="btn-quickSearch" title="Quick Search" value="Quick Search" type="submit" />
                    <%= link_to "Quick search", transactions_path, :class => "btn-quickSearch", :title => "Quick Search", :value => "Quick Search", :type => "submit" %>
                    <input class="btn-advanceSearch active" title="Advance search" value="Advance search" type="submit" />
                    <input class="btn-filter" title="Filter" value="Filter" type="submit" />
                    <ul class="list-advanceSearch">
                      <%= form_for '', :url => transaction_advance_search_transactions_path, :method => "GET", :remote => true do |f| %>
                        <li>
                          <p>Advance Search</p>
                          <input title="Advance Search" placeholder="Search by Address, Agents or MLS" type="text" />
                        </li>
                        <%= f.text_field :close_date %>
                        <%= f.submit "search" %>
                        <li>
                          <p>Close date start (m/d/yyyy)</p>
                          <input class="icon-closedate" title="Close date start (m/d/yyyy)" type="text" />
                        </li>
                        <li>
                          <p>Expiry date start (m/d/yyyy)</p>
                          <input class="icon-closedate" title="Expiry date start (m/d/yyyy)" type="text" />
                        </li>
                        <li>
                          <p>Status</p>
                          <input title="Any Status" placeholder="Any Status" type="text" />
                        </li>
                        <li>
                          <p>Close date end (m/d/yyyy)</p>
                          <input class="icon-closedate" title="Close date end (m/d/yyyy)" type="text" />
                        </li>
                        <li>
                          <p>Expiry date start (m/d/yyyy)</p>
                          <input class="icon-closedate" title="Expiry date start (m/d/yyyy)" type="text" />
                        </li>
                      <% end %>
                    </ul>
                    <input class="btn-searchTransactions btn-advancesearch" title="Search Transactions" value="Search Transactions" type="submit" />
                    <div class="clearFix"></div>
                  </div>
                </div>
                <div class="officerow">Only active transactions are shown below. <a class="link-viewall" title="View all &rarr;" href="#">View all &rarr;</a></div>
                <div class="contentbox">
                  <h1>
                    <div class="column w30">Transaction</div>
                    <div class="column">Status</div>
                    <div class="column w11">Complete</div>
                    <div class="column w9">Agents</div>
                    <div class="column w20">Location</div>
                    <div class="column">Close Date</div>
                    <div class="column last">Actions</div>
                  </h1>
                  <div id="advance_search">
                    <div class="box">
                      <div class="tableTransaction">
                        <div class="transactionrow">
                          <% @transactions.each do |transaction| %>
                          <div class="column w30"><%= transaction.transaction_name %></div>
                          <div class="column"><%= transaction.status %></div>
                          <div class="column w11">
                            <div class="completebox"><abbr>7/8</abbr></div>
                            <img alt="" title="" src="/assets/spacer.png" />
                          </div>
                          <div class="column w9"><div class="icon-agent"></div></div>
                          <div class="column w20">Elite Real Estate</div>
                          <div class="column">Dec 14, 12</div>
                          <div class="column last">
                            <span class="btn-action">
                              <em class="tooltip-container"><span>Action</span>
                                <strong class="tooltip">
                                  <ul class="list-tooltip">
                                    <li class="icon-zoomin"><a title="View Transaction" href="#">View Transaction</a></li>
                                    <li class="icon-edit"><a title="Edit Transactionn" href="#">Edit Transaction</a></li>
                                  </ul>
                                  <i class="top-arrow"></i>
                                </strong>
                              </em>
                            </span>
                          </div>
                          <% end %>
                        </div>


                      </div>
                    </div>

                  </div>

                </div>
                <div class="clearFix"></div>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
    <div class="clearFix"></div>
  </div>
</div>
<div class="clearFix"></div>
</div>

</div>
<%#*<script type="text/javascript" language="javascript" src="js/jquery_002.js"></script>%>
<script type="text/javascript" language="javascript">
  jQuery(document).ready(function () {
    jQuery('#leftColumnShow').click(function () {
      jQuery('#leftColumn').slideToggle('slow', function () {})
    });
    jQuery('#closeBox').click(function () {
      jQuery('#leftColumn').slideToggle('slow', function () {})
    });
  });
</script>
</body>
</html>